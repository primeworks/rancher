index="sfx" source="*-Diesel_Efficiency.csv"  Distance > 500 | lookup "drivers" "Driver ID" as "Driver ID" OUTPUT "First Name" "Last Name" | rename "Fuel Saved" as Fuel_Saved, "EST Idle Fuel" as Idle_Fuel | rex field=Fuel_Saved mode=sed "s/,//g" | eval Driver='First Name'+" "+'Last Name', Distance=tonumber(Distance), "Fuel Saved"=tonumber(Fuel_Saved), "EST Idle Fuel"=tonumber(Idle_Fuel) | rename Distance as "Distance Travelled", "EST Idle Fuel" as "Idle Fuel" | search "Driver" != NULL
